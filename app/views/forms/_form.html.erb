<%= div_for form do %>
    <div class="row">
    <div class="col-md-1"><%= form.title %></div>
    <div class="col-md-1"><%= form.email %></div>
    <div class="col-md-1"><%= form.text %></div>
    <div class="col-md-1"><%= link_to 'edit', edit_form_path(form) %></div>
    <div class="col-md-1"><%= link_to 'show', form_path(form) %></div>
    <div class="col-md-1"> <%= link_to 'delete', form, method: :delete, remote: true %></div>
    </div>
<% end %>
